{
  "name": "awezapopup",
  "version": "3.1.3",
  "description": "The Aweza popup for web",
  "main": "dist/index.js",
  "author": "Werner Roets <werner@fastacademy.co.za>",
  "license": "GPL-3.0-only",
  "scripts": {
    "production": "yarn transpile && node_modules/.bin/parcel build index.jsx --out-file aweza-popup.min.js --public-url . && node_modules/.bin/csso style.css --output dist/aweza-popup.min.css",
    "dev": "./dev.sh",
    "transpile": "babel index.jsx -d dist --copy-files",
    "test": "jest",
    "lint": "node_modules/.bin/eslint src/ index.jsx",
    "fix": "node_modules/.bin/eslint --fix src/ index.jsx"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "csso-cli": "^1.1.0",
    "eslint": "^5.9.0",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.9.7",
    "preact-render-to-json": "^3.6.6"
  },
  "dependencies": {
    "preact": "^8.3.1",
    "tippy.js": "^2.5.4"
  },
  "jest": {
    "testRegex": "(/__tests__/.*(\\.|/)(test))\\.(js|jsx)?$",
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ]
  }
}
